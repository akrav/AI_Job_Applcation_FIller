# Copy this file to .env and fill values

# App
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# Supabase API (Project settings -> API)
SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
SUPABASE_ANON_KEY=your-anon-key
# Server-side only: keep private
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Postgres (Supabase -> Database -> Connection String)
# Example: postgresql://postgres:PASSWORD@db.PROJECT_REF.supabase.co:5432/postgres?sslmode=require
DATABASE_URL=postgresql://postgres:YOUR_PASSWORD@db.YOUR_PROJECT_REF.supabase.co:5432/postgres?sslmode=require

# Compatibility vars for scripts
DB_HOST=db.YOUR_PROJECT_REF.supabase.co
DB_ANON_KEY=${SUPABASE_ANON_KEY}

# JWT for any custom signing in backend (if used)
JWT_SECRET=replace-with-a-long-random-string

# External APIs (optional for future tickets)
OPENAI_API_KEY=